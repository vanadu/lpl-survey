{
  "name": "ASPIRATION-page",
  "elements": [
    {
      "type": "panel",
      "name": "AspirationPanel",
      "elements": [
        {
          "type": "html",
          "name": "AspirationHeading",
          "html": "<h3>{CmpnName}'s experience with aspiration pneumonia</h3>"
        },
        {
          "type": "html",
          "name": "AspirationText",
          "html": "<p>Aspiration pneumonia (AP) is the most common and most serious direct complication of LP/GOLPP. This sections asks questions about {CmpnName}'s experience with AP. </p>"
          
        },
        {
          "type": "html",
          "name": "LandingData",
          "html": "<div class='srvy-general-info'><img src='/img-info-icon.png' class='srvy-info-icon' alt='Example image' />&nbsp;<a href='https://example.com' class='srvy-info-link' target='_blank' rel='noopener noreferrer'>What is aspiration pneumonia as why is it a frequent complication of LP/GOLPP ?</a></div>"
        },

        {
          "type": "radiogroup",
          "name": "AspirationDidHave",
          "title": "Did {CmpnName} ever get aspiration pneumonia (AP) after the appearance of the first LP/GOLPP symptoms?",
          "isRequired": true,
          "choices": [
            {
              "value": "YES",
              "text": "Yes"
            },
            {
              "value": "NO",
              "text": "No"
            },
            {
              "value": "DONT_KNOW",
              "text": "Don't know"
            }
          ],
          "showOtherItem": false
        },



        {
          "type": "panel",
          "name": "AspirationGeneralPanel",
          "visibleIf": "{AspirationDidHave} = 'YES'",
          "elements": [
            {
            "type": "dropdown",
            "name": "AspirationCount",
            "title": "How many times did {CmpnName} get AP?",
            "isRequired": true,
            "visibleIf": "{AspirationDidHave} = 'YES'",
            "choices": [
              {
                "value": "ONE",
                "text": "Once"
              },
              {
                "value": "TWO",
                "text": "Twice"
              },
              {
                "value": "THREE",
                "text": "Three times"
              },
              {
                "value": "MORE_THAN_THREE",
                "text": "More than three times"
              }
            ],
            "showOtherItem": false
            },
            {
              "type": "radiogroup",
              "name": "AspirationInpatient",
              "title": "Did {CmpnName} ever require inpatient, overnight, or emergency care for any of these occurrences of AP?",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },
            {
              "type": "dropdown",
              "name": "AspirationInpatientCount",
              "visibleIf": "{AspirationInpatient} = 'YES'",
              "title": "How many times did {CmpnName} require inpatient, overnight, or emergency care for AP?",
              "isRequired": true,
              "choices": [
                {
                  "value": "ONE",
                  "text": "Once"
                },
                {
                  "value": "TWO",
                  "text": "Twice"
                },
                {
                  "value": "THREE",
                  "text": "Three times"
                },
                {
                  "value": "MORE_THAN_THREE",
                  "text": "More than three times"
                }
              ],
              "showOtherItem": false
            }
          ]
        },
        {
          "type": "panel",
          "name": "AspirationSurgeryPanel",
          "visibleIf": "{TreatmentStatusDidHave} = 'SURGERY' && {AspirationDidHave} = YES",
          "elements": [

            {
              "type": "radiogroup",
              "name": "AspirationSurgeryPost",
              "title": "Did {CmpnName} ever have AP after the LP surgery (i.e., tieback, BVEAP)?",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },

            {
              "type": "dropdown",
              "name": "AspirationSurgeryPostCount",
              "title": "In the course of {CmpnName}'s life post-op, how many times did {CmpnName} get AP?",
              "isRequired": true,
              "visibleIf": "{AspirationSurgeryPost} = YES",
              "choices": [
                {
                  "value": "ONE",
                  "text": "Once"
                },
                {
                  "value": "TWO",
                  "text": "Twice"
                },
                {
                  "value": "THREE",
                  "text": "Three times"
                },
                {
                  "value": "MORE_THAN_THREE",
                  "text": "More than three times"
                }
              ],
              "showOtherItem": false
            }
    
          ]
        },


        {
          "type": "panel",
          "name": "AspirationStentPanel",
          "visibleIf": "{TreatmentStatusDidHave} = 'STENT' && {AspirationDidHave} = YES",
          "elements": [

            {
              "type": "radiogroup",
              "name": "AspirationStentPost",
              "title": "Did {CmpnName} ever have AP after the stent implant procedure?",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },

            {
              "type": "dropdown",
              "name": "AspirationStentPostCount",
              "title": "In the course of {CmpnName}'s life after the stent implant, how many times did {CmpnName} get AP?",
              "isRequired": true,
              "visibleIf": "{AspirationSurgeryPost} = YES",
              "choices": [
                {
                  "value": "ONE",
                  "text": "Once"
                },
                {
                  "value": "TWO",
                  "text": "Twice"
                },
                {
                  "value": "THREE",
                  "text": "Three times"
                },
                {
                  "value": "MORE_THAN_THREE",
                  "text": "More than three times"
                }
              ],
              "showOtherItem": false
            }
    
          ]
        },
        



        {
          "type": "panel",
          "name": "AspirationCostPanel",
          "visibleIf": "{AspirationDidHave} = YES",
          "elements": [
            {
            "type": "dropdown",
            "name": "AspirationCost",
            "title": "What was the approximate pre-insurance cost of the AP treatment for {CmpnName}:",
            "isRequired": true,
            "choices": [
              {
                "value": "UNDER_500",
                "text": "Under $500"
              },
              {
                "value": "501-1000",
                "text": "$501-$1000"
              },
              {
                "value": "1001-2000",
                "text": "$1001-$2000"
              },
              {
                "value": "2001-3000",
                "text": "$2001-$3000"
              },
              {
                "value": "3001-5000",
                "text": "$3001-$5000"
              },
              {
                "value": "5001-8000",
                "text": "$5001-$8000"
              },
              {
                "value": "OVER_8001",
                "text": "Over $8001"
              }
            ],
            "showOtherItem": true,
            "showNoneItem": true,
            "required": true
          }



    
          ]
        }
      ]
    }
  ]
}