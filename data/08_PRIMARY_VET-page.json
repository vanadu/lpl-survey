{
  "name": "PRIMARY_VET-page",
  "elements": [
    {
      "type": "panel",
      "name": "PrimaryVetPanel",
      "elements": [
        {
          "type": "html",
          "name": "PrimaryVetHeading",
          "html": "<h3>Your primary veterinarian and {CmpnName}'s LP/GOLPP</h3>"
        },
        {
          "type": "html",
          "name": "PrimaryVetText",
          "html": "<p>This section asks questions about your primary veterinarian's handling of {CmpnName}'s LP/GOLPP symptoms.</p><p>If you changed primary vets since {CmpnName}'s LP/GOLPP symptoms first appeared   , please answer the question about your preferred or most recent primary veterinarian."
        },
        {
          "type": "panel",
          "name": "PrimaryVetInflammationPanel",
          "title": "",
          "elements": [
            {
              "type": "radiogroup",
              "name": "PrimaryVetInflammation",
              "title": "Did your primary veterinarian observe any inflammation, irritation, or infection in the larynx/pharynx area?",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            }
          ]
        },
        {
          "type": "panel",
          "name": "PrimaryVetDiagnosisPanel",
          "elements": [
            {
              "type": "radiogroup",
              "name": "PrimaryVetDiagnosis",
              "title": "Did your primary vet give you a =i issue oncediagnosis of LP for {CmpnName}? ",
              "isRequired": true,
              "choices": [
                {
                  "value": "NO_MENTION",
                  "text": "No, did not mention LP/GOLPP at all."
                },
                {
                  "value": "NO_SUGGESTED",
                  "text": "Suggested {CmpnName}'s symptoms might be due to LP/GOLPP but did not give a definitive diagnosis."
                },
                {
                  "value": "YES_UNCONFIRMED",
                  "text": "Provided a definitive diagnosis of LP/GOLPP but recommended confirming with a specialist."
                },
                {
                  "value": "YES_CONFIRMED",
                  "text": "Provided definitive diagnosis and did not recommend confirmation by a specialist."
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },
            {
              "type": "radiogroup",
              "name": "PrimaryVetDiagnosticType",
              "title": "What diagnostic procedure did your primary veterinarian use to arrive at their conclusion?",
              "isRequired": true,
              "visibleIf": "{PrimaryVetDiagnosis} = 'NO_SUGGESTED' || {PrimaryVetDiagnosis} = 'YES_UNCONFIRMED' || {PrimaryVetDiagnosis} = 'YES_CONFIRMED' ",
              "choices": [
                {
                  "value": "ROUTINE_EXAM",
                  "text": "Routine exam"
                },
                {
                  "value": "SEDATED_VISUAL",
                  "text": "Visual exam under sedation"
                },
                {
                  "value": "XRAY",
                  "text": "X-Ray or other imaging"
                },
                {
                  "value": "ULTRASOUND",
                  "text": "Ultrasound"
                },
                {
                  "value": "SCOPE",
                  "text": "Video endoscope or laryngoscope under anesthesia"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": true
            }
    
          ]
        },
        {
          "type": "panel",
          "name": "panelName",
          "elements": [

            {
              "type": "radiogroup",
              "name": "PrimaryVetPrescriptions",
              "title": "Did your primary veterinarian provide any prescription medications for {CmpnName}'s LP/GOLPP?",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },
            {
              "type": "checkbox",
              "name": "PrimaryVetPrescriptionType",
              "title": "What prescription medications if any did your primary veterinarian prescribe or recommend for {CmpnName}'s LP/GOLPP?",
              "isRequired": true,
              "visibleIf": "{PrimaryVetPrescriptions} = YES",
              "choices": [
                {
                  "value": "DOXEPIN",
                  "text": "Doxepin"
                },
                {
                  "value": "TRAZODONE",
                  "text": "Trazodone"
                },
                {
                  "value": "GABAPENTIN",
                  "text": "Gabapentin"
                },
                {
                  "value": "STERIOD",
                  "text": "Prednisone or other steriod"
                },
                {
                  "value": "NSAID",
                  "text": "Carprofen or other NSAID"
                },
                {
                  "value": "LIBRELA",
                  "text": "Librela/Beransa"
                },
                {
                  "value": "CERENIA",
                  "text": "Cerenia (maripotant citrate)"
                },
                {
                  "value": "MELOXICAM",
                  "text": "Meloxicam"
                },
                {
                  "value": "ACEPROMAZINE",
                  "text": "Acepromazine (Atravet, ACP)"
                },
                {
                  "value": "ALBUTEROL",
                  "text": "Albuterol or other inhaler"
                },
                {
                  "value": "TEMARIL P",
                  "text": "Temaril P"
                }
              ],
              "showOtherItem": true,
              "showNoneItem": true
            },
            {
              "type": "rating",
              "name": "PrimaryVetPrescriptionEffectiveness",
              "title": "How effective were these prescription medications for {CmpnName}'s LP/GOLPP?",
              "description": "1 = not at all effective, 5 = very effective",
              "isRequired": true,
              "visibleIf": "{PrimaryVetPrescriptions} = YES",
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "text": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" },
                { "value": "DONT_KNOW", "text": "Don't know" }
              ]
            }
          ]
        },
        {
          "type": "panel",
          "name": "PrimaryVetNonPrescriptionPanel",
          "elements": [

            {
              "type": "radiogroup",
              "name": "PrimaryVetNonPrescription",
              "title": "Not including over-the-counter (OTC) supplements, did your current primary veterinarian provide or recommend any non-prescription treatments or therapies for {CmpnName}'s LP/GOLPP?:",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },
            {
              "type": "checkbox",
              "name": "PrimaryVetNonPrescriptionType",
              "title": "What non-prescription treatments (not including OTC supplements) did your current primary veterinarian recommend?",
              "isRequired": true,
              "visibleIf": "{PrimaryVetNonPrescription} = YES",
              "choices": [
                {
                  "value": "ACUPUNCTURE",
                  "text": "Acupuncture"
                },
                {
                  "value": "PHYSICAL_THERAPY",
                  "text": "Physical or hydro therapy"
                },
                {
                  "value": "LLLT",
                  "text": "Cold laser or red light therapy"
                }
              ],
              "showOtherItem": true
            },
            {
              "type": "rating",
              "name": "PrimaryVetNonPrescriptionEffectiveness",
              "title": "How effective were these treatments?",
              "description": "1 = not at all effective, 5 = very effective",
              "isRequired": true,
              "visibleIf": "{PrimaryVetNonPrescription} = YES",
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "text": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" },
                { "value": "DONT_KNOW", "text": "Don't know" }
              ]
            }
          ]
        }
      ]
    }
  ]
}
