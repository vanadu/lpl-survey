{
  "name": "PRIMARY_VET-page",
  "elements": [
    {
      "type": "panel",
      "name": "PrimaryVetPanel",
      "elements": [
        {
          "type": "html",
          "name": "PrimaryVetHeading",
          "html": "<h3>Your primary veterinarian and {CmpnName}'s LP/GOLPP</h3>"
        },
        {
          "type": "html",
          "name": "PrimaryVetText",
          "html": "<p>This section asks questions about your primary veterinarian's handling of {CmpnName}'s LP/GOLPP symptoms.</p><p>If you changed primary vets since {CmpnName}'s LP/GOLPP symptoms first appeared   , please answer the question about your preferred or most recent primary veterinarian."
        },
        {
          "type": "panel",
          "name": "PrimaryVetInflammationPanel",
          "title": "",
          "elements": [
            {
              "type": "radiogroup",
              "name": "PrimaryVetInflammation",
              "title": "Did your primary veterinarian observe any inflammation, irritation, or infection in the larynx/pharynx area?",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            }
          ]
        },
        {
          "type": "panel",
          "name": "PrimaryVetDiagnosisPanel",
          "elements": [
            {
              "type": "radiogroup",
              "name": "PrimaryVetDiagnosis",
              "title": "Did your primary vet discuss LP/GOLPP {CmpnName}'s as a possible cause of {CmpnName}'s symptoms? ",
              "isRequired": true,
              "choices": [
                {
                  "value": "NO_MENTION",
                  "text": "No, did not mention LP/GOLPP at all"
                },
                {
                  "value": "POSSIBLY",
                  "text": "Said {CmpnName}'s symptoms were possibly due to LP/GOLPP"
                },
                {
                  "value": "DEFINITELY",
                  "text": "Said {CmpnName}'s symptoms were definitely due to LP/GOLPP"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            }
          ]
        },

        {
          "type": "panel",
          "name": "PrimaryRecommendationInfoPanel",
          "title": "",
          "elements": [
          {
            "type": "boolean",
            "name": "PrimaryRecommendInfoDidGive",
            "title": "Did your current primary veterinarian give you helpful information about how LP/GOLPP might affect {CmpnName}?",
            "isRequired": true
          },

          {
            "type": "checkbox",
            "name": "PrimaryRecommendInfoDetails",
            "title": "What did they tell you about the how LP/GOLPP might affect {CmpnName}?",
            "isRequired": true,
            "visibleIf": "{PrimaryRecommendInfoDidGive} = true",
            "choices": [
              {
                "value": "BARK_LOSS",
                "text": "Loss or weakening of bark"
              },
              {
                "value": "PROGRESSIVE",
                "text": "LP/GOLPP is a progressive disease"
              },
              {
                "value": "HOT_HUMID",
                "text": "Hot, humid weather exacerbates symptoms"
              },
              {
                "value": "BREATHING_CRISIS",
                "text": "Potential for life-threatening breathing crisis"
              }
            ],
            "showOtherItem": true
          }
        ]
        },
        {
          "type": "panel",
          "name": "PrimaryRecommendationSurgeryPanel",
          "title": "",
          "elements": [

            {
              "type": "radiogroup",
              "name": "PrimaryRecommendSurgery",
              "title": "Did your primary veterinarian make a recommendation about tieback or other LP/GOLPP surgeries for {CmpnName}'s LP/GOLPP?",
              "isRequired": true,
              "choices": [
                {
                  "value": "NO_RECOMMENDATION",
                  "text": "Made no recommendation"
                },
                {
                  "value": "ADVISED_FOR",
                  "text": "Encouraged me to talk to a surgeon"
                },
                {
                  "value": "ADVISED_AGAINST",
                  "text": "Advised against surgery"
                }
              ],
              "showOtherItem": false
            },
            {
              "type": "radiogroup",
              "name": "PrimaryRecommendSurgeryPerception",
              "title": "Do you feel like your primary veterinarian's recommendation against surgery was due to {CmpnName}'s condition or to a generally negative perception of tieback surgery?",
              "isRequired": true,
              "visibleIf": "{PrimaryRecommendSurgery} = 'ADVISED_AGAINST'",
              "choices": [
                {
                  "value": "SPECIFIC_CONDITION",
                  "text": "{CmpnName}'s specific condition"
                },
                {
                  "value": "GENERAL_PERCEPTION",
                  "text": "General negative perception"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            }
          ]
        },
        {
          "type": "panel",
          "name": "PrimaryRecommendationConfidencePanel",
          "title": "",
          "elements": [


            {
              "type": "rating",
              "name": "PrimaryRecommendConfidence",
              "title": "How confident have you been you in your current primary veterinarian's knowledge level regarding LP/GOLPP?",
              "description": "1 = not at all confident, 5 = very confident",
              "isRequired": true,
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "text": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" },
                { "value": "DONT_KNOW", "text": "Don't know" }
              ]
            },
            {
              "type": "rating",
              "name": "PrimaryRecommendCSatisfaction",
              "title": "How satisfied have you been with your current primary veterinarian's handling of {CmpnName}'s LP/GOLPP?",
              "description": "1 = not at all satisfied, 5 = very satisfied",
              "isRequired": true,
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "tex  !expectt": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" },
                { "value": "DONT_KNOW", "text": "Don't know" }
              ]
            }
          ]
        },





        {
          "type": "panel",
          "name": "panelName",
          "elements": [

            {
              "type": "radiogroup",
              "name": "PrimaryVetPrescriptions",
              "title": "Did your primary veterinarian provide any prescription medications for {CmpnName}'s LP/GOLPP?",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },
            {
              "type": "checkbox",
              "name": "PrimaryVetPrescriptionType",
              "title": "What prescription medications if any did your primary veterinarian prescribe or recommend for {CmpnName}'s LP/GOLPP?",
              "isRequired": true,
              "visibleIf": "{PrimaryVetPrescriptions} = YES",
              "choices": [
                {
                  "value": "DOXEPIN",
                  "text": "Doxepin"
                },
                {
                  "value": "TRAZODONE",
                  "text": "Trazodone"
                },
                {
                  "value": "GABAPENTIN",
                  "text": "Gabapentin"
                },
                {
                  "value": "STERIOD",
                  "text": "Prednisone or other steriod"
                },
                {
                  "value": "NSAID",
                  "text": "Carprofen or other NSAID"
                },
                {
                  "value": "LIBRELA",
                  "text": "Librela/Beransa"
                },
                {
                  "value": "CERENIA",
                  "text": "Cerenia (maripotant citrate)"
                },
                {
                  "value": "MELOXICAM",
                  "text": "Meloxicam"
                },
                {
                  "value": "ACEPROMAZINE",
                  "text": "Acepromazine (Atravet, ACP)"
                },
                {
                  "value": "ALBUTEROL",
                  "text": "Albuterol or other inhaler"
                },
                {
                  "value": "TEMARIL P",
                  "text": "Temaril P"
                }
              ],
              "showOtherItem": true,
              "showNoneItem": true
            },
            {
              "type": "rating",
              "name": "PrimaryVetPrescriptionEffectiveness",
              "title": "How effective were these prescription medications for {CmpnName}'s LP/GOLPP?",
              "description": "1 = not at all effective, 5 = very effective",
              "isRequired": true,
              "visibleIf": "{PrimaryVetPrescriptions} = YES",
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "text": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" },
                { "value": "DONT_KNOW", "text": "Don't know" }
              ]
            }
          ]
        },
        {
          "type": "panel",
          "name": "PrimaryVetNonPrescriptionPanel",
          "elements": [

            {
              "type": "radiogroup",
              "name": "PrimaryVetNonPrescription",
              "title": "Not including over-the-counter (OTC) supplements, did your current primary veterinarian provide or recommend any non-prescription treatments or therapies for {CmpnName}'s LP/GOLPP?:",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },
            {
              "type": "checkbox",
              "name": "PrimaryVetNonPrescriptionType",
              "title": "What non-prescription treatments (not including OTC supplements) did your current primary veterinarian recommend?",
              "isRequired": true,
              "visibleIf": "{PrimaryVetNonPrescription} = YES",
              "choices": [
                {
                  "value": "ACUPUNCTURE",
                  "text": "Acupuncture"
                },
                {
                  "value": "PHYSICAL_THERAPY",
                  "text": "Physical or hydro therapy"
                },
                {
                  "value": "LLLT",
                  "text": "Cold laser or red light therapy"
                }
              ],
              "showOtherItem": true
            },
            {
              "type": "rating",
              "name": "PrimaryVetNonPrescriptionEffectiveness",
              "title": "How effective were these treatments?",
              "description": "1 = not at all effective, 5 = very effective",
              "isRequired": true,
              "visibleIf": "{PrimaryVetNonPrescription} = YES",
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "text": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" },
                { "value": "DONT_KNOW", "text": "Don't know" }
              ]
            }
          ]
        }
      ]
    }
  ]
}
