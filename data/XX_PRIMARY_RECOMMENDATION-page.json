{
  "name": "PRIMARY_RECOMMENDATION-page",
  "elements": [
    {
      "type": "panel",
      "name": "PrimaryRecommendationPanel",
      "elements": [
        {
          "type": "html",
          "name": "PrimaryRecommendationHeading",
          "html": "<h3>This section asks about specific recommendations your primary made about {CmpnName}'s LP/GOLPP symptoms.</h3>"
        },
        {
          "type": "panel",
          "name": "PrimaryRecommendationInfoPanel",
          "title": "",
          "elements": [
          {
            "type": "boolean",
            "name": "PrimaryRecommendInfoDidGive",
            "title": "Did your current primary veterinarian give you helpful information about how LP/GOLPP might affect {CmpnName}?",
            "isRequired": true
          },

          {
            "type": "checkbox",
            "name": "PrimaryRecommendInfoDetails",
            "title": "What did they tell you about the how LP/GOLPP might affect {CmpnName}?",
            "isRequired": true,
            "visibleIf": "{PrimaryRecommendInfoDidGive} = true",
            "choices": [
              {
                "value": "BARK_LOSS",
                "text": "Loss or weakening of bark"
              },
              {
                "value": "PROGRESSIVE",
                "text": "LP/GOLPP is a progressive disease"
              },
              {
                "value": "HOT_HUMID",
                "text": "Hot, humid weather exacerbates symptoms"
              },
              {
                "value": "BREATHING_CRISIS",
                "text": "Potential for life-threatening breathing crisis"
              }
            ],
            "showOtherItem": true
          }
        ]
        },
        {
          "type": "panel",
          "name": "PrimaryRecommendationSurgeryPanel",
          "title": "",
          "elements": [

            {
              "type": "radiogroup",
              "name": "PrimaryRecommendSurgery",
              "title": "Did your primary veterinarian make a recommendation about tieback or other LP/GOLPP surgeries for {CmpnName}'s LP/GOLPP?",
              "isRequired": true,
              "choices": [
                {
                  "value": "NO_RECOMMENDATION",
                  "text": "Made no recommendation"
                },
                {
                  "value": "ADVISED_FOR",
                  "text": "Encouraged me to talk to a surgeon"
                },
                {
                  "value": "ADVISED_AGAINST",
                  "text": "Advised against surgery"
                }
              ],
              "showOtherItem": false
            },
            {
              "type": "radiogroup",
              "name": "PrimaryRecommendSurgeryPerception",
              "title": "Do you feel like your primary veterinarian's recommendation against surgery was due to {CmpnName}'s condition or to a generally negative perception of tieback surgery?",
              "isRequired": true,
              "visibleIf": "{PrimaryRecommendSurgery} = 'ADVISED_AGAINST'",
              "choices": [
                {
                  "value": "SPECIFIC_CONDITION",
                  "text": "{CmpnName}'s specific condition"
                },
                {
                  "value": "GENERAL_PERCEPTION",
                  "text": "General negative perception"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            }
          ]
        },




        {
          "type": "panel",
          "name": "PrimaryRecommendationConfidencePanel",
          "title": "",
          "elements": [


            {
              "type": "rating",
              "name": "PrimaryRecommendConfidence",
              "title": "How confident have you been you in your current primary veterinarian's knowledge level regarding LP/GOLPP?",
              "description": "1 = not at all confident, 5 = very confident",
              "isRequired": true,
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "text": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" },
                { "value": "DONT_KNOW", "text": "Don't know" }
              ]
            },
            {
              "type": "rating",
              "name": "PrimaryRecommendCSatisfaction",
              "title": "How satisfied have you been with your current primary veterinarian's handling of {CmpnName}'s LP/GOLPP?",
              "description": "1 = not at all satisfied, 5 = very satisfied",
              "isRequired": true,
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "tex  !expectt": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" },
                { "value": "DONT_KNOW", "text": "Don't know" }
              ]
            }
          ]
        }
      ]
    }
  ]
}