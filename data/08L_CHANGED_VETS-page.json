{
  "name": "L_CHANGED_VETS-page",
  "elements": [
    {
      "type": "panel",
      "name": "lvngChangedVetsPanel",
      "elements": [
        {
          "type": "html",
          "name": "lvngChangedVetsHeading",
          "html": "<h3>Change of veterinarians due to LP/GOLPP</h3>"
        },
        {
          "type": "html",
          "name": "lvngChangedVetsText",
          "html": "<p>This section asks whether you changed primary veterinarians because of how {CmpnName}'s LP/GOLPP symptoms were handled and why you made that decision. </p>"
        },
        {
          "type": "panel",
          "name": "lvngChangedVetsDetailsPanel",
          "title": "",
          "elements": [


            {
              "type": "radiogroup",
              "name": "lvngChangedVets",
              "title": "Did you ever change primary veterinarians because how they handled {CmpnName}'s LP/GOLPP symptoms?",
              "isRequired": true,
              "choices": [
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "ONCE",
                  "text": "Once"
                },
                {
                  "value": "MORE_THAN_ONCE",
                  "text": "More than once"
                }
              ]
            },

            {
              "type": "checkbox",
              "name": "lvngChangedVetsReason",
              "title": "Why did you change veterinarians after {CmpnName} started showing symptoms of LP/GOLPP (please check all that apply)?",
              "isRequired": true,
              "visibleIf": "{lvngChangedVets} != NO",
              "choices": [
                {
                  "value": "NO_KNOWLEDGE",
                  "text": "The previous vet couldn't give possible causes for {CmpnName}'s symptoms."
                },
                {
                  "value": "NOT_SERIOUS",
                  "text": "The previous vet didn't take {CmpnName}'s symptoms seriously."
                },
                {
                  "value": "AGE_ONLY",
                  "text": "The previous vet said {CmpnName}'s symptoms were just due to age."
                },
                {
                  "value": "BAD_TESTS",
                  "text": "The previous vet ordered tests that I didn't think were relevant to {CmpnName}'s symptoms."
                },
                {
                  "value": "NO_VALUE",
                  "text": "I wasn't getting enough value from the previous vet's handling of {CmpnName}'s symptoms."
                },
                {
                  "value": "MISDIAGNOSED",
                  "text": "The previous vet misdiagnosed {CmpnName}'s symptoms."
                }
              ],
              "showOtherItem": true
            },
            {
              "type": "rating",
              "name": "lvngChangeVetsConfidence",
              "title": "How confident were you in your previous primary vet's knowledge level about LP/GOLPP?",
              "description": "1 = no confident at all, 5 = very confident",
              "isRequired": true,
              "visibleIf": "{lvngChangedVets} != NO",
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "text": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" }

              ]
            },

            {
              "type": "radiogroup",
              "name": "lvngChangedVetsAdvice",
              "title": "What advice or recommendation did your previous primary vet's give you advice about dogname's LP/GOLPP symptoms?",
              "isRequired": true,
              "visibleIf": "{lvngChangedVets} != NO",
              "choices": [
                {
                  "value": "NOTHING_CAN_BE_DONE",
                  "text": "Explicitly said nothing can be done about {CmpnName}'s symptoms"
                },
                {
                  "value": "RECOMMENDED_SPECIALIST",
                  "text": "Recommended consulting a specialist"
                },
                {
                  "value": "RECOMMENDED_EUTHANASIA",
                  "text": "Recommended euthanasia"
                }
              ],
              "showOtherItem": true,
              "showNoneItem": true
            },

            {
              "type": "radiogroup",
              "name": "lvngChangedVetsSpecialistRecommendation",
              "title": "What kind of specialist did your previous vet recommend or refer?",
              "isRequired": true,
              "visibleIf": "{lvngChangedVetsAdvice} = 'RECOMMENDED_SPECIALIST'",
              "choices": [
                {
                  "value": "AIRWAY_SURGEON",
                  "text": "Surgeon experienced in tieback or other LP surgery"
                },
                {
                  "value": "ORTHOPEDIC_SURGEON",
                  "text": "General or orthopedic surgeon"
                },
                {
                  "value": "NEUROLOGIST",
                  "text": "Neurologist"
                },
                {
                  "value": "STENT_PROVIDER",
                  "text": "Stent provider"
                }
              ],
              "showOtherItem": true,
              "otherText": "Other"
            }
          ]
        }
      ]
    }
  ]
}
