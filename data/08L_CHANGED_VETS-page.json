{
  "name": "08L_CHANGED_VETS-page",
  "elements": [
    {
      "type": "panel",
      "name": "lvngChangedVetsPanel",
      "elements": [
        {
          "type": "html",
          "name": "lvngChangedVetsHeading",
          "html": "<h3>Did you change vets?(LP)</h3>"
        },
        {
          "type": "html",
          "name": "lvngChangedVetsText",
          "html": "<p>Tell us why you changed vets.</p>"
        },
        {
          "type": "panel",
          "name": "lvngChangedVetsDetailsPanel",
          "title": "",
          "elements": [


            {
              "type": "radiogroup",
              "name": "lvngChangedVets",
              "title": "Did you ever change primary veterinarians because how they handled {CmpnName}'s LP/GOLPP symptoms?",
              "isRequired": true,
              "choices": [
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "ONCE",
                  "text": "Once"
                },
                {
                  "value": "MORE_THAN_ONCE",
                  "text": "More than once"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ]
            },

            {
              "type": "checkbox",
              "name": "lvngChangedVetsReason",
              "title": "Why did you change veterinarians after {CmpnName} started showing symptoms of LP/GOLPP?:",
              "isRequired": true,
              "choices": [
                {
                  "value": "NOT_SERIOUS",
                  "text": "ITEM_1"
                },
                {
                  "value": "NOT_INTERESTED",
                  "text": "ITEM_2"
                },
                {
                  "value": "AGE_ONLY",
                  "text": "ITEM_3"
                },
                {
                  "value": "BAD_TESTS",
                  "text": "OTHER"
                },
                {
                  "value": "NO_VALUE",
                  "text": "OTHER"
                },
                {
                  "value": "MISDIAGNOSED",
                  "text": "OTHER"
                },
                {
                  "value": "OTHER",
                  "text": "Other (max. 250 characters)"
                }

              ],
              "showOtherItem": true
            },

            {
              "type": "radio",
              "name": "lvngChangedVetsAdvice",
              "title": "What, if any, advice did your previous primary vet/s give you advice about dogname's LP/GOLPP symptoms?",
              "isRequired": true,
              "choices": [
                {
                  "value": "NOTHING_CAN_BE_DONE",
                  "text": "Said nothing can be done"
                },
                {
                  "value": "INEFFECTIVE_TREATMENTS",
                  "text": "Provided ineffective treatments"
                },
                {
                  "value": "RECOMMENDED_SPECIALIST",
                  "text": "Recommended consulting a specialist"
                },
                {
                  "value": "RECOMMENDED_EUTHANASIA",
                  "text": "Recommended euthanasia"
                },
                {
                  "value": "OTHER",
                  "text": "Other (max. 250 characters)"
                }
              ],
              "showOtherItem": true,
              "showNoneItem": true
            },

            {
              "type": "radiogroup",
              "name": "lvngChangedVetsSpecialistRecommendation",
              "title": "What kind of specialist did your previous vet recommend or refer?",
              "isRequired": true,
              "visibleIf": "{lvngChangedVetsAdvice} = 'RECOMMENDED_SPECIALIST'",
              "choices": [
                {
                  "value": "AIRWAY_SURGEON",
                  "text": "Surgeon experienced in tieback or other LP surgery"
                },
                {
                  "value": "ORTHOPEDIC_SURGEON",
                  "text": "General or orthopedic surgeon"
                },
                {
                  "value": "NEUROLOGIST",
                  "text": "NEUROLOGIST"
                },
                {
                  "value": "STENT_PROVIDER",
                  "text": "Stent provider"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": true,
              "otherText": "Other"
            },
            {
              "type": "rating",
              "name": "lvngChangeVetsConfidence",
              "title": "How confident were you in your previous primary vet/s' knowledge level regarding LP/GOLPP?",
              "description": "1 = no confident at all, 5 = very confident",
              "isRequired": true,
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "text": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" },
                { "value": "DONT_KNOW", "text": "Don't know" }
              ]
            }
          ]
        }
      ]
    }
  ]
}
