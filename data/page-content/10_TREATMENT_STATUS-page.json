              "type": "dropdown",
              "name": "TreatmentStatusTiebackWait",
              "title": "How long did you have to wait for non-emergency tieback surgery for {CmpnName}:",
              "isRequired": true,
              "visibleIf": "{TreatmentStatusTiebackEmergency} = true",
              "choices": [
                {
                  "value": "UNDER_ONE",
                  "text": "Less than one month"
                },
                {
                  "value": "ONE_TWO",
                  "text": "Between one and two months"
                },
                {
                  "value": "THREE_SIX",
                  "text": "Between three and six months"
                },
                {
                  "value": "MORE_THAN_SIX",
                  "text": "Longer than six months"
                }
              ],
              "showOtherItem": true,
              "showNoneItem": true,
              "required": true
            },

            {
              "type": "radiogroup",
              "name": "TreatmentStatusTiebackCount",
              "title": "How many times did {CmpnName} undergo tieback (UAL) surgery?",
              "isRequired": true,
