{
  "name": "TREATMENT_STATUS-page",
  "elements": [
    {
      "type": "panel",
      "name": "TreatmentStatusPanel",
      "elements": [
        {
          "type": "html",
          "name": "TreatmentStatusHeading",
          "html": "<h3>Veterinary-Medical Procedures</h3>"
        },
        {
          "type": "html",
          "name": "TreatmentStatusText",
          "html": "<p>This section asks questions about any surgery or implant procedure {CmpnName} underwent to treat LP/GOLPP symptoms. This only includes surgeries or procedures that diriectly modify the structure of the larynx.</p>"
        },

        {
          "type": "radiogroup",
          "name": "TreatmentStatusDidHave",
          "title": "Did {CmpnName} undergo a veterinary-medical procedure such as surgery or a stent implant for LP/GOLPP?",
          "isRequired": true,
          "choices": [
            {
              "value": true,
              "text": "Yes"
            },
            {
              "value": false,
              "text": "No"
            }
          ],
          "showOtherItem": false
        },
        {
          "type": "panel",
          "name": "TreatmentStatusAgePanel",
          "title": "At about what age did {CmpnName} have the first surgery or stent procedure?",
          "description": "If you can't remember exactly, it's OK to guesstimate.",
          "visibleIf": "{TreatmentStatusDidHave} = true",
          "elements": [
            {
              "type": "text",
              "name": "TreatmentStatusAgeYears",
              "title": "Age (years)",
              "isRequired": true
            },
            {
              "type": "text",
              "name": "TreatmentStatusAgeMonths",
              "title": "Age (months)",
              "isRequired": true
            }
          ]
        },

        {
          "type": "panel",
          "name": "TreatmentStatusTypePanel",
          "elements": [
            {
            "type": "checkbox",
            "name": "TreatmentStatusType",
            "title": "What kind procedure or procedures did {CmpnName} have?",
            "description": "Please check all that apply.",
            "isRequired": true,
            "visibleIf": "{TreatmentStatusDidHave} = true",
            "choices": [
              {
                "value": "TIEBACK",
                "text": "Tieback (Unilateral Arytenoid Lateralization)"
              },
              {
                "value": "BVEAP",
                "text": "BVEAP (Bilateral Vocal Fold Excision & Arytenoid Pexy)"
              },
              {
                "value": "STENT",
                "text": "Laryngeal stent"
              },
              { "value": "ARYTENOIDECTOMY",
                "text": "Partial arytenoidectomy"  
              },
              { "value": "VENTRILOCORDECTOMY",
                "text": "Ventrilocordectomy"  
              }
            ],
            "showOtherItem": true
            }
          ]
        },
