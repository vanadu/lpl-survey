{
  "name": "L_PRIMARY_VET-page",
  "elements": [
    {
      "type": "panel",
      "name": "lvngPrimaryVetPanel",
      "elements": [
        {
          "type": "html",
          "name": "lvngPrimaryVetHeading",
          "html": "<h3>Your current primary veterinarians and {CmpnName}'s LP/GOLPP</h3>"
        },
        {
          "type": "html",
          "name": "lvngPrimaryVetText",
          "html": "<p>This section asks questions about how your current primary veterinarian has handled {CmpnName}'s LP/GOLPP journey.</p>"
        },
        {
          "type": "panel",
          "name": "lvngPrimaryVetDetailsPanel",
          "title": "",
          "elements": [



            {
              "type": "radiogroup",
              "name": "lvngPrimaryVetDiagnosis",
              "title": "Did your current primary vet give you a diagnosis of LP for {CmpnName}? ",
              "isRequired": true,
              "choices": [
                {
                  "value": "NO_MENTION",
                  "text": "No, did not mention LP/GOLPP at all."
                },
                {
                  "value": "NO_SUGGESTED",
                  "text": "Suggested {CmpnName}'s symptoms might be due to LP/GOLPP but did not give a definitive diagnosis."
                },
                {
                  "value": "YES_UNCONFIRMED",
                  "text": "Provided a definitive diagnosis of LP/GOLPP but recommended confirming with a specialist."
                },
                {
                  "value": "YES_CONFIRMED",
                  "text": "Provided definitive diagnosis and did not recommend confirmation by a specialist."
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },

            {
              "type": "radiogroup",
              "name": "lvngPrimaryVetDiagnosticType",
              "title": "What diagnostic procedure did your current primary veterinarian use to arrive at their conclusion?",
              "isRequired": true,
              "visibleIf": "{lvngPrimaryVetDiagnosis} != 'NO_MENTION'",
              "choices": [
                {
                  "value": "ROUTINE_EXAM",
                  "text": "Routine exam"
                },
                {
                  "value": "SEDATED_VISUAL",
                  "text": "Visual exam under sedation"
                },
                {
                  "value": "XRAY",
                  "text": "X-Ray or other imaging"
                },
                {
                  "value": "ULTRASOUND",
                  "text": "Ultrasound"
                },
                {
                  "value": "SCOPE",
                  "text": "Video endoscope or laryngoscope under anesthesia"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": true
            },
            {
              "type": "radiogroup",
              "name": "lvngPrimaryVetInflammation",
              "title": "Did your current primary veterinarian observe any inflammation, irritation, or infection in the larynx/pharynx area?",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },
            {
              "type": "radiogroup",
              "name": "lvngPrimaryVetPrescriptions",
              "title": "Did your current primary veterinarian provide any prescription medications for {CmpnName}'s LP/GOLPP?",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },
            {
              "type": "checkbox",
              "name": "lvngPrimaryVetPrescriptionType",
              "title": "What prescription medications if any did your primary veterinarian prescribe or recommend for {CmpnName}'s LP/GOLPP?",
              "isRequired": true,
              "visibleIf": "{lvngPrimaryVetPrescriptions} = YES",
              "choices": [
                {
                  "value": "DOXEPIN",
                  "text": "Doxepin"
                },
                {
                  "value": "TRAZODONE",
                  "text": "Trazodone"
                },
                {
                  "value": "GABAPENTIN",
                  "text": "Gabapentin"
                },
                {
                  "value": "STERIOD",
                  "text": "Prednisone or other steriod"
                },
                {
                  "value": "NSAID",
                  "text": "Carprofen or other NSAID"
                },
                {
                  "value": "LIBRELA",
                  "text": "Librela/Beransa"
                },
                {
                  "value": "CERENIA",
                  "text": "Cerenia (maripotant citrate)"
                },
                {
                  "value": "MELOXICAM",
                  "text": "Meloxicam"
                },
                {
                  "value": "ACEPROMAZINE",
                  "text": "Acepromazine (Atravet, ACP)"
                },
                {
                  "value": "ALBUTEROL",
                  "text": "Albuterol or other inhaler"
                },
                {
                  "value": "TEMARIL P",
                  "text": "Temaril P"
                }
              ],
              "showOtherItem": true,
              "showNoneItem": true
            },
            {
              "type": "rating",
              "name": "lvngPrimaryVetPrescriptionEffectiveness",
              "title": "How effective were these prescription medicatons for {CmpnName}'s LP/GOLPP?",
              "description": "1 = not at all effective, 5 = very effective",
              "isRequired": true,
              "visibleIf": "{lvngPrimaryVetPrescriptions} = YES",
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "text": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" },
                { "value": "DONT_KNOW", "text": "Don't know" }
              ]
            },

            {
              "type": "radiogroup",
              "name": "lvngPrimaryVetNonPrescription",
              "title": "Not including OTC supplements, did your current primary veterinarian provide or recommend any non-prescription treatments or therapies for {CmpnName}'s LP/GOLPP?:",
              "isRequired": true,
              "choices": [
                {
                  "value": "YES",
                  "text": "Yes"
                },
                {
                  "value": "NO",
                  "text": "No"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },

            {
              "type": "checkbox",
              "name": "lvngPrimaryVetNonPrescriptionType",
              "title": "What non-prescription treatments (not including OTC supplements) did your current primary veterinarian recommend?",
              "isRequired": true,
              "visibleIf": "{lvngPrimaryVetNonPrescription} = YES",
              "choices": [
                {
                  "value": "ACUPUNCTURE",
                  "text": "Acupuncture"
                },
                {
                  "value": "PHYSICAL_THERAPY",
                  "text": "Physical or hydro therapy"
                },
                {
                  "value": "LLLT",
                  "text": "Cold laser or red light therapy"
                },
                {
                  "value": "COUPAGE",
                  "text": "Coupage"
                },
                {
                  "value": "Other",
                  "text": "OTHER"
                }
              ],
              "showOtherItem": true
            },
            {
              "type": "rating",
              "name": "lvngPrimaryVetNonPrescriptionEffectiveness",
              "title": "How effective were these treatments?",
              "description": "1 = not at all effective, 5 = very effective",
              "isRequired": true,
              "visibleIf": "{lvngPrimaryVetNonPrescription} = YES",
              "rateValues": [
                { "value": 1, "text": "1" },
                { "value": 2, "text": "2" },
                { "value": 3, "text": "3" },
                { "value": 4, "text": "4" },
                { "value": 5, "text": "5" },
                { "value": "DONT_KNOW", "text": "Don't know" }
              ]
            }
          ]
        }
      ]
    }
  ]
}
