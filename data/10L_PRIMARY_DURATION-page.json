{
  "name": "L_PRIMARY_DURATION-page",
  "elements": [
    {
      "type": "panel",
      "name": "lvngPrimaryDurationPanel",
      "elements": [
        {
          "type": "html",
          "name": "lvngPrimaryDurationHeading",
          "html": "<h3>Duration of care by primary veterinarian for LP/GOLPP symptoms</h3>"
        },
        {
          "type": "html",
          "name": "lvngPrimaryDurationText",
          "html": "<p>This section asks questions about how long {CmpnName} has been in the care of primary veterinarians for LP/GOLPP symptoms and how {CmpnName}'s condition changed over this time period. This includes your current primary veterinarian and any prior vets who examined {CmpnName} with LP/GOLPP symptoms.</p>"
        },
        {
          "type": "panel",
          "name": "lvngPrimaryDurationFirstConsult",
          "title": "About how old was {CmpnName} when you first consulted with a primary veterinarian about LP/GOLPP symptoms?",
          "elements": [
            {
              "type": "text",
              "inputType": "number",
              "name": "lvngPrimaryDurationFirstConsultYears",
              "title": "Age in years (number):",
              "isRequired": true,
              "min": "1",
              "max": "40",
              "validators": [
                {
                  "type": "numeric",
                  "minValue": 0,
                  "maxValue": 40,
                  "text": "Age must be between 1 and 40"
                }
              ]
            },
            {
              "type": "text",
              "inputType": "number",
              "name": "lvngPrimaryDurationFirstConsultMonths",
              "title": "Months (number 1 - 12)",
              "isRequired": true,
              "min": 1,
              "max": 12,
              "validators": [
                {
                  "type": "numeric",
                  "minValue": 0,
                  "maxValue": 12,
                  "text": "Months must be between 1 and 12"
                }
              ]
            }
          ]
        },

        {
          "type": "panel",
          "name": "lvngPrimaryDurationVisitsNumberPanel",
          "elements": [
            {
              "type": "radiogroup",
              "name": "lvngPrimaryDurationVisitsNumber",
              "title": "About how many times have you visited a primary veterinarian (including your current one) about {CmpnName}'s LP/GOLPP symptoms?",
              "isRequired": true,
              "choices": [
                {
                  "value": "ONE",
                  "text": "Once"
                },
                {
                  "value": "TWO",
                  "text": "Twice"
                },
                {
                  "value": "MORE_THAN_TWO",
                  "text": "More than twice"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know"
                }
              ],
              "showOtherItem": false
            },
            {
              "type": "radiogroup",
              "name": "lvngPrimaryDurationCost",
              "title": "About what was the total pre-insurance cost of these visits to primary veterinarians about dogname's LP/GOLPP symptoms?",
              "isRequired": true,
              "choices": [
                {
                  "value": "UNDER_1000",
                  "text": "Under $1000"
                },
                {
                  "value": "1000_3000",
                  "text": "$1000-$3000"
                },
                {
                  "value": "3000_5000",
                  "text": "$3000-$5000"
                },
                {
                  "value": "OVER_5000",
                  "text": "Over $5000"
                },
                {
                  "value": "DONT_KNOW",
                  "text": "Don't know or prefer not to say"
                }
              ],
              "showOtherItem": false
            }
          ]
        }
      ]
    }
  ]
}
